name: new_tinder_clone
description: A Flutter Tinder clone application.
publish_to: 'none'
version: 1.0.0+1

environment:
  sdk: '>=3.0.0 <4.0.0'

dependencies:
  flutter:
    sdk: flutter
  flutter_native_splash: ^2.3.6
  provider: ^6.0.5
  shared_preferences: ^2.2.1
  intl: ^0.18.1
  cached_network_image: ^3.2.3
  firebase_core: 2.15.1
  firebase_auth: 4.6.3
  cloud_firestore: 4.8.1
  firebase_storage: 11.2.3
  # firebase_messaging: 14.6.4
  cloud_functions: ^4.4.0  # Fixed name
  image_picker: ^0.8.9
  # geolocator: ^10.0.1
  # geocoding: ^2.1.1
  uuid: ^3.0.7
  google_fonts: ^5.1.0
  confetti: ^0.7.0
  shimmer: ^3.0.0
  flutter_facebook_auth: ^6.0.3
  http: ^1.0.0
  timeago: ^3.5.0
  country_code_picker: ^3.0.0
  cloudinary_public: ^0.21.0
  google_sign_in: 6.1.0


flutter_native_splash:
  color: "#000000"
  image: assets/logo.png
  android: true
  ios: true




dev_dependencies:
  flutter_test:
    sdk: flutter
  flutter_lints: ^3.0.0

flutter:
